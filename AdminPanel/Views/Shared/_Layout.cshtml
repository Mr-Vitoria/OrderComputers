<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AdminPanel</title>
    <link rel="stylesheet" href="~/AdminPanel.styles.css" asp-append-version="true" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body id="body-pd">
    <header class="header" id="header">
        <div class="header_toggle"> <i class="bi bi-list" id="header-toggle"></i> </div>
        <div class="header_img"> <img src="https://i.imgur.com/hczKIze.jpg" alt=""> </div>
    </header>
    <div class="l-navbar" id="nav-bar">
        <nav class="nav">
            <div>
                <a asp-action="Index" asp-controller="AdminPanel" class="nav_logo"><span class="nav_logo-name">OuTouch</span> </a>
                <div class="nav_list">
                    <a asp-action="Index" asp-controller="AdminPanel" class="nav_link active">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Главная</span>
                    </a>
                    <a asp-action="Index" asp-controller="CompBodies" class="nav_link">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Корпусы</span>
                    </a>
                    <a asp-action="Index" asp-controller="CompProcessors" class="nav_link">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Процессоры</span>
                    </a>
                    <a asp-action="Index" asp-controller="ComputerAssemblies" class="nav_link">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Сборки</span>
                    </a>
                    <a asp-action="Index" asp-controller="MotherCards" class="nav_link">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Материнские платы</span>
                    </a>
                    <a asp-action="Index" asp-controller="PowerSupplyUnits" class="nav_link">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Блок питания</span>
                    </a>
                    <a asp-action="Index" asp-controller="RAMMemories" class="nav_link">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Оперативная память</span>
                    </a>
                    <a asp-action="Index" asp-controller="StorageDevices" class="nav_link">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Запоминающие устройства</span>
                    </a>
                    <a asp-action="Index" asp-controller="VideoCards" class="nav_link">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Видеокарты</span>
                    </a>
                    <a asp-action="Index" asp-controller="Users" class="nav_link">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Пользователи</span>
                    </a>
                    <a asp-action="Index" asp-controller="Orders" class="nav_link">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Заказы</span>
                    </a>
                </div>
            </div> <a asp-action="SignOut" asp-controller="AdminPanel" class="nav_link"> <i class='bx bx-log-out nav_icon'></i> <span class="nav_name">Выход</span> </a>
        </nav>
    </div>
    <div class="container">
        <main role="main" class="pb-3 bg-light">
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe" crossorigin="anonymous"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
